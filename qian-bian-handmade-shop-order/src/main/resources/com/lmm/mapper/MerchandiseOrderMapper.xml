<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lmm.mapper.MerchandiseOrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.lmm.entity.MerchandiseOrder">
        <id column="id" property="id"/>
        <result column="user_id" property="userId"/>
        <result column="shop_id" property="shopId"/>
        <result column="merchandises" property="merchandises"/>
        <result column="state" property="state"/>
        <result column="delivery_address" property="deliveryAddress"/>
        <result column="created_time" property="createdTime"/>
        <result column="total" property="total"/>
        <result column="actual_total" property="actualTotal"/>
        <result column="voucher_id" property="voucherId"/>
        <result column="pay_time" property="payTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, user_id, shop_id, merchandises, state, delivery_address, created_time, total, actual_total, voucher_id, pay_time
    </sql>

</mapper>
